    "# 908. 最小差值 I\n",
    "class Solution:\n",
    "    def smallestRangeI(self, A: List[int], K: int) -> int:\n",
    "        a = max(A)\n",
    "        i = min(A)\n",
    "        if a - i <= 2 * K:\n",
    "            return 0\n",
    "        else:\n",
    "            return (a - i) - 2 * K"
   ]
  }
